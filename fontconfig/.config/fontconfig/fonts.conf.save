<!DOCTYPE fontconfig SYSTEM 'fonts.dtd'>
<fontconfig>
<!-- Set preferred serif, sans serif, and monospace fonts. -->
  <alias>
    <family>serif</family>
    <prefer><family>SF Pro Rounded</family></prefer>
  </alias>
  <alias>
    <family>sans-serif</family>
    <prefer><family>SF Pro Rounded</family></prefer>
  </alias>
  <alias>
    <family>sans</family>
    <prefer><family>SF Pro Rounded</family></prefer>
  </alias>
  <alias>
    <family>monospace</family>
    <prefer><family>Input</family></prefer>
  </alias>
</fontconfig>
